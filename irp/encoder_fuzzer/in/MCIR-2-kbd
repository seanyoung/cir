{300,msb}<-1,1|1,-1>(9,32:8,C:5,0:8,F:8,M:8,-74m)*
{c1 =  #(F & 0b11111000) % 2,
 c2 = (#(F & 0b00000111) + #(M & 0b00110000)) % 2,
 c3 = (#(F & 0b11000111) + #(M & 0b10001110)) % 2,
 c4 = (#(F & 0b00110110) + #(M & 0b10101101)) % 2,
 c5 = (#(F & 0b10101101) + #(M & 0b10011011)) % 2,
 C = (c1 << 4) | (c2 << 3) | (c3 << 2) | (c4 << 1) | c5 }
[F:0..255,M:0..255]